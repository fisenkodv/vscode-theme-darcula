{
    "type": "dark",
    "name": "Darcula",
    "semanticHighlighting": true,
    "colors": {
        "editor.background": "#2B2B2B",
        "editor.foreground": "#CCCCCC",
        "editor.selectionBackground": "#204182CC",
        "editor.lineHighlightBackground": "#FFFFFF0B",
        "editorGutter.background": "#FFFFFF0B",
        "editorLineNumber.activeForeground": "#A4A3A3",
        "editorLineNumber.foreground": "#606366",

        "titleBar.border": "#323232",
        "titleBar.activeBackground": "#3C3F41",
        "titleBar.inactiveBackground": "#FFFFFF10",
        "titleBar.activeForeground": "#CCCCCC",

        "sideBar.border": "#FFFFFF1A",
        "sideBar.background": "#3C3F41",
        "sideBar.foreground": "#BBBBBB",

        "sideBarTitle.foreground": "#BBBBBB",
        "sideBarSectionHeader.background": "#3C3F41",
        "sideBarSectionHeader.border": "#323232",
        "sideBarSectionHeader.foreground": "#BBBBBB",

        "statusBar.foreground": "#CCCCCC",
        "statusBar.background": "#3C3F41",
        "statusBar.border": "#464646",

        "tab.activeBackground": "#4E5254",
        "tab.inactiveBackground": "#3C3F41",
        "tab.activeBorder": "#4A88C7",

        "editorSuggestWidget.background": "#303030",
        "editorSuggestWidget.highlightForeground": "#CC6E2F",
        "editorSuggestWidget.selectedBackground": "#204182CC",

        "badge.background": "#FF00001A",

        "activityBar.activeBackground": "#3472ED",
        "activityBar.background": "#3C3F41",
        "activityBar.border": "#323232",
        "activityBar.foreground": "#FFFFFF",
        "activityBar.inactiveForeground": "#CED0D6",

        "activityBarBadge.background": "#548AF7",
        "activityBarBadge.foreground": "#CED0D6",

        "list.hoverBackground": "#45494A",
        "list.hoverForeground": "#BBBBBB",

        "scrollbarSlider.background": "#494949",
        "scrollbarSlider.activeBackground": "#595959",

        "tree.indentGuidesStroke": "#505355",
        "tree.inactiveIndentGuidesStroke": "#505355",

        "editorWhitespace.foreground": "#5A5A5A",
        "editorRuler.foreground": "#4D4D4D",

        "editorInlayHint.foreground": "#787878",
        "editorInlayHint.typeForeground": "#787878"
    },
    "tokenColors": [
        {
            "scope": [
                "keyword.control.import",
                "keyword.control.from",
                "keyword.control.as",
                "keyword.control.export",
                "keyword.control.loop",
                "keyword.operator.expression.of",
                "keyword.control.conditional",
                "keyword.control.switch",
                "keyword.control.flow",
                "keyword.operator.new",
                "keyword.control.trycatch",
                "entity.name.function,meta.function-call,new.expr"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#597CC2"
            }
        },
        {
            "scope": ["meta.definition.function"],
            "settings": {
                "fontStyle": "",
                "foreground": "#D9AF6C"
            }
        },
        {
            "scope": ["meta.function-call"],
            "settings": {
                "fontStyle": "",
                "foreground": "#FFC66D"
            }
        },
        {
            "scope": ["variable.other.constant", "meta.embedded.line", "meta.template.expression", "string.template"],
            "settings": {
                "fontStyle": "",
                "foreground": "#A9B7C6"
            }
        },

        {
            "scope": ["string.template", "string.quoted.single"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#807D6E"
            }
        },
        {
            "scope": ["storage.modifier.async", "storage.type", "storage.type.function"],
            "settings": {
                "fontStyle": "",
                "foreground": "#597CC2"
            }
        },
        {
            "scope": ["comment.block.documentation"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#7A7A7A"
            }
        },
        {
            "scope": ["comment.line.double-slash"],
            "settings": {
                "foreground": "#7A7A7A"
            }
        },
        {
            "scope": ["storage.type.class.jsdoc"],
            "settings": {
                "fontStyle": "italic bold",
                "foreground": "#7A7A7A"
            }
        },
        {
            "scope": "variable.other.jsdoc",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#7584A1"
            }
        },

        {
            "scope": ["variable.other.readwrite.alias"],
            "settings": {
                "fontStyle": "",
                "foreground": "#A9B7C6"
            }
        },
        {
            "scope": ["meta.import"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#807D6E"
            }
        },
        {
            "scope": [
                "meta.brace.round",
                "meta.arrow",
                "punctuation.definition.block",
                "punctuation.definition.parameters.begin",
                "punctuation.definition.parameters.end",
                "punctuation.definition.block",
                "punctuation.terminator.statement"
            ],
            "settings": {
                "foreground": "#AEB5BD"
            }
        },
        {
            "scope": ["constant.numeric.decimal"],
            "settings": {
                "foreground": "#267DFF"
            }
        },
        {
            "scope": "support.type.property-name.json",
            "settings": {
                "foreground": "#7F8EBE"
            }
        },
        {
            "scope": "string.quoted.double.json",
            "settings": {
                "foreground": "#817D6C"
            }
        },
        {
            "scope": "constant.language.json",
            "settings": {
                "foreground": "#4E7DC8"
            }
        },
        {
            "scope": "constant.numeric.json",
            "settings": {
                "foreground": "#007FFF"
            }
        },
        {
            "scope": [
                "punctuation.definition.dictionary.begin.json",
                "punctuation.definition.dictionary.end.json",
                "punctuation.definition.array.begin.json",
                "punctuation.definition.array.end.json"
            ],
            "settings": {
                "foreground": "#ACB5BE"
            }
        },
        {
            "scope": ["source.json.comments"],
            "settings": {
                "foreground": "#7A7A7A"
            }
        },
        {
            "scope": ["variable.other.constant.property"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#7A84AC"
            }
        },
        {
            "scope": ["punctuation.terminator.statement", "punctuation.separator.comma"],
            "settings": {
                "foreground": "#597CC2"
            }
        },
        {
            "scope": ["variable.parameter"],
            "settings": {
                "foreground": "#A9B7C6"
            }
        },
        {
            "scope": ["meta.object-literal.key"],
            "settings": {
                "foreground": "#828EBA"
            }
        },
        {
            "scope": ["entity.name.tag.yaml", "constant.language.boolean.yaml"],
            "settings": {
                "foreground": "#597CC2"
            }
        },
        {
            "scope": ["string.unquoted.plain.out.yaml"],
            "settings": {
                "foreground": "#AEB5BD"
            }
        },
        {
            "scope": ["string.quoted.double.yaml"],
            "settings": {
                "foreground": "#807D6E"
            }
        }
    ]
}
